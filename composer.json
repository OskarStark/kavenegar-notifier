{
    "name": "aalinia/kavenegar-notifier",
    "type": "symfony-bridge",
    "description": "Symfony Kavenegar Notifier Bridge",
    "keywords": ["sms", "kavenegar", "notifier"],
    "homepage": "https://symfony.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Alireza Alinia",
            "email": "a.alinia@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "symfony/http-client": "^4.3|^5.0",
        "symfony/notifier": "^5.2"
    },
    "autoload": {
        "psr-4": { "Symfony\\Component\\Notifier\\Bridge\\Kavenegar\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "5.2-dev"
        }
    }
}
